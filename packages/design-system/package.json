{
  "name": "@note-taking-app/design-system",
  "version": "1.0.0",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    "./styles.css": "./dist/styles.css",
    "./theme.css": "./src/theme.css",
    "./logo-icon.tsx": "./src/components/logo-icon.tsx",
    "./home-icon.tsx": "./src/components/home-icon.tsx",
    "./settings-icon.tsx": "./src/components/settings-icon.tsx",
    "./tag-icon.tsx": "./src/components/tag-icon.tsx",
    "./archive-icon.tsx": "./src/components/archive-icon.tsx",
    "./search-icon.tsx": "./src/components/search-icon.tsx",
    "./arrow-icon.tsx": "./src/components/arrow-icon.tsx",
    "./sun-icon.tsx": "./src/components/sun-icon.tsx",
    "./moon-icon.tsx": "./src/components/moon-icon.tsx",
    "./logout-icon.tsx": "./src/components/logout-icon.tsx",
    "./lock-icon.tsx": "./src/components/lock-icon.tsx",
    "./font-icon.tsx": "./src/components/font-icon.tsx",
    "./system-icon.tsx": "./src/components/system-icon.tsx"
  },
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "lint": "eslint src/",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "type-check": "tsc --noEmit"
  },
  "packageManager": "pnpm@10.7.1",
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@note-taking-app/eslint-config": "workspace:*",
    "@note-taking-app/typescript-config": "workspace:*",
    "@types/react": "^19.1.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.4",
    "@tailwindcss/postcss": "^4.1.4",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4"
  }
}
